{
  "name": "workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "add:app": "nx g @nrwl/nest:app",
    "add:m": "nx g @nrwl/nest:module",
    "add:c": "nx g @nrwl/nest:controller",
    "add:s": "nx g @nrwl/nest:service",
    "add:f": "nx g @nrwl/nest:filter",
    "add:i": "nx g @nrwl/nest:interceptor",
    "add:g": "nx g @nrwl/nest:guard",
    "add:lib": "nx g @nrwl/workspace:library",
    "add:nest:lib": "nx g @nrwl/nest:lib",
    "build": "nx run-many --target=build --all --prod --verbose",
    "start": "nx run-many --target=serve --all --parallel=100 --host=0.0.0.0",
    "debug": "skaffold dev",
    "test": "nx run-many --all --target=test --runInBand --verbose",
    "test:docker": "docker compose -f docker-compose.dev.yml run mc-v2-test yarn test --skip-nx-cache",
    "test:watch": "nx run-many --all --target=test --watch --runInBand --verbose --host=0.0.0.0",
    "test:serve:cov": "http-server coverage",
    "lint": "nx run-many --all --target=lint --fix",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:db:init": "docker exec mc-v2-mongodb-lynx sh /data/scripts/init.sh /bin/bash",
    "docs": "compodoc -p tsconfig.base.json -s",
    "husky:prepare": "husky install",
    "spell": "cspell \"**\" --gitignore --no-summary --no-progress --no-must-find-files",
    "spell:in": "cspell stdin"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.4.6",
    "@nestjs/mongoose": "^9.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "mongodb-memory-server": "^8.9.3",
    "mongoose": "^6.3.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.3.0",
    "@nrwl/nest": "14.1.9",
    "nx": "14.1.9",
    "typescript": "~4.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/config-nx-scopes": "^17.0.0",
    "@compodoc/compodoc": "^1.1.19",
    "@cspell/dict-es-es": "^2.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/node": "14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cspell": "^6.2.3",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jest": "27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1"
  }
}
